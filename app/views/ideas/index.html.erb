<% @ideas.each do |idea| %>
  <ul id="each-idea">
      <h2><%= idea.title %></h2>
      <li><%= idea.body %></li>
      <li><%= idea.quality %></li>
      <li><button type="button">Update</button></li>
  </ul>
<% end %>

<h1>Have Another Idea?</h1>
  <div class="form">
    <form>
      Title:<br>
      <input type="text" name="title" id="idea-title"><br>
      Body:<br>
      <input type="text" name="body" id="idea-body"><br><br>
      <input type="submit" value="Save" id="create-idea">
    </form>
  </div>
